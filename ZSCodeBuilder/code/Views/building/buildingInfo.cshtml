@model Model.tb_building
<div class="main-content">
    <div class="main-content-inner">
        <div class="page-content main-content">
            <div class="breadcrumbs ace-save-state" id="breadcrumbs">
                <ul class="breadcrumb">
                    <li>
                        <i class="ace-icon fa fa-home home-icon"></i>
                        <a href="/">首页</a>
                    </li>
                    <li>
                        <a href="/building/buildingList">楼宇</a>
                    </li>
                    <li class="active">
                        楼宇详情
</li>
                </ul>
                <!-- /.breadcrumb -->
            </div>

            <div class="row">
                <div class="col-xs-12">
                    <div class="widget-box">
                        <div class="widget-header">
                            <h4 class="widget-title">基本信息</h4>

                            <div class="widget-toolbar">
                                <a href="#" data-action="collapse">
                                    <i class="ace-icon fa fa-chevron-up"></i>
                                </a>
                            </div>
                        </div>

                        <div class="widget-body" style="display: block;">
                            <div class="widget-main">
                                	<form action="/building/buildingSave" class="form-horizontal" role="form">
		<input type="hidden" name="id" value="@Model.id" />
		<div class="form-group">
			<label class="col-xs-2 control-label " >楼宇名称</label>
			<div class="col-xs-4"><input type="text" id="buildingname" name="buildingname" class="col-xs-10 col-sm-10 " value="@Model.buildingname"></div>
			<label class="col-xs-2 control-label " >行政区域</label>
			<div class="col-xs-4"><input type="text" id="administrative" name="administrative" class="col-xs-10 col-sm-10 " value="@Model.administrative"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >省</label>
			<div class="col-xs-4"><input type="text" id="provinceid" name="provinceid" class="col-xs-10 col-sm-10 " value="@Model.provinceid"></div>
			<label class="col-xs-2 control-label " >市</label>
			<div class="col-xs-4"><input type="text" id="cityid" name="cityid" class="col-xs-10 col-sm-10 " value="@Model.cityid"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >县</label>
			<div class="col-xs-4"><input type="text" id="countyid" name="countyid" class="col-xs-10 col-sm-10 " value="@Model.countyid"></div>
			<label class="col-xs-2 control-label " >详细地址</label>
			<div class="col-xs-4"><input type="text" id="address" name="address" class="col-xs-10 col-sm-10 " value="@Model.address"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >物业公司</label>
			<div class="col-xs-4"><input type="text" id="propertycompany" name="propertycompany" class="col-xs-10 col-sm-10 " value="@Model.propertycompany"></div>
			<label class="col-xs-2 control-label " >物业公司简介</label>
			<div class="col-xs-4"><input type="text" id="propertycompanyintro" name="propertycompanyintro" class="col-xs-10 col-sm-10 " value="@Model.propertycompanyintro"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >交通信息</label>
			<div class="col-xs-4"><input type="text" id="traffic" name="traffic" class="col-xs-10 col-sm-10 " value="@Model.traffic"></div>
			<label class="col-xs-2 control-label " >客服电话</label>
			<div class="col-xs-4"><input type="text" id="customer" name="customer" class="col-xs-10 col-sm-10 " value="@Model.customer"></div>
		</div>
		<div class="clearfix form-actions"><div class="col-md-offset-5 col-md-9"><button class="btn btn-info" type="button" onclick="zstools.save()"><i class="ace-icon fa fa-check bigger-110"></i>Save（保存）</button></div></div>
	</form>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- /.row -->
    </div>
    <!-- /.page-content -->
</div>
 