@model Model.tb_roomlease
<div class="main-content">
    <div class="main-content-inner">
        <div class="page-content main-content">
            <div class="breadcrumbs ace-save-state" id="breadcrumbs">
                <ul class="breadcrumb">
                    <li>
                        <i class="ace-icon fa fa-home home-icon"></i>
                        <a href="/">首页</a>
                    </li>
                    <li>
                        <a href="/roomlease/roomleaseList">租赁情况</a>
                    </li>
                    <li class="active">
                        租赁情况详情
</li>
                </ul>
                <!-- /.breadcrumb -->
            </div>

            <div class="row">
                <div class="col-xs-12">
                    <div class="widget-box">
                        <div class="widget-header">
                            <h4 class="widget-title">基本信息</h4>

                            <div class="widget-toolbar">
                                <a href="#" data-action="collapse">
                                    <i class="ace-icon fa fa-chevron-up"></i>
                                </a>
                            </div>
                        </div>

                        <div class="widget-body" style="display: block;">
                            <div class="widget-main">
                                	<form action="/roomlease/roomleaseSave" class="form-horizontal" role="form">
		<input type="hidden" name="id" value="@Model.id" />
		<div class="form-group">
			<label class="col-xs-2 control-label " >房间ID</label>
			<div class="col-xs-4"><input type="text" id="roomid" name="roomid" class="col-xs-10 col-sm-10 " value="@Model.roomid"></div>
			<label class="col-xs-2 control-label " >租赁情况</label>
			<div class="col-xs-4"><input type="text" id="company" name="company" class="col-xs-10 col-sm-10 " value="@Model.company"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >租赁周期</label>
			<div class="col-xs-4"><input type="text" id="time" name="time" class="col-xs-10 col-sm-10 " value="@Model.time"></div>
			<label class="col-xs-2 control-label " >联系人</label>
			<div class="col-xs-4"><input type="text" id="linkman" name="linkman" class="col-xs-10 col-sm-10 " value="@Model.linkman"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >联系电话</label>
			<div class="col-xs-4"><input type="text" id="phone" name="phone" class="col-xs-10 col-sm-10 " value="@Model.phone"></div>
			<label class="col-xs-2 control-label " >紧急联系人</label>
			<div class="col-xs-4"><input type="text" id="emergencylinkman" name="emergencylinkman" class="col-xs-10 col-sm-10 " value="@Model.emergencylinkman"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >紧急联系电话</label>
			<div class="col-xs-4"><input type="text" id="emergencyphone" name="emergencyphone" class="col-xs-10 col-sm-10 " value="@Model.emergencyphone"></div>
		</div>
		<div class="clearfix form-actions"><div class="col-md-offset-5 col-md-9"><button class="btn btn-info" type="button" onclick="zstools.save()"><i class="ace-icon fa fa-check bigger-110"></i>Save（保存）</button></div></div>
	</form>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- /.row -->
    </div>
    <!-- /.page-content -->
</div>
 