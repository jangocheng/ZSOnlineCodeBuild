@model Model.tb_weibolicense
<div class="main-content">
    <div class="main-content-inner">
        <div class="page-content main-content">
            <div class="breadcrumbs ace-save-state" id="breadcrumbs">
                <ul class="breadcrumb">
                    <li>
                        <i class="ace-icon fa fa-home home-icon"></i>
                        <a href="/">首页</a>
                    </li>
                    <li>
                        <a href="/weibolicense/weibolicenseList">微博</a>
                    </li>
                    <li class="active">
                        微博详情
</li>
                </ul>
                <!-- /.breadcrumb -->
            </div>

            <div class="row">
                <div class="col-xs-12">
                    <div class="widget-box">
                        <div class="widget-header">
                            <h4 class="widget-title">基本信息</h4>

                            <div class="widget-toolbar">
                                <a href="#" data-action="collapse">
                                    <i class="ace-icon fa fa-chevron-up"></i>
                                </a>
                            </div>
                        </div>

                        <div class="widget-body" style="display: block;">
                            <div class="widget-main">
                                	<form action="/weibolicense/weibolicenseSave" class="form-horizontal" role="form">
		<input type="hidden" name="id" value="@Model.id" />
		<div class="form-group">
			<label class="col-xs-2 control-label " >公众号ID</label>
			<div class="col-xs-4"><input type="text" id="openid" name="openid" class="col-xs-10 col-sm-10 " value="@Model.openid"></div>
			<label class="col-xs-2 control-label " >订阅信息</label>
			<div class="col-xs-4"><input type="text" id="subscribe" name="subscribe" class="col-xs-10 col-sm-10 " value="@Model.subscribe"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >昵称</label>
			<div class="col-xs-4"><input type="text" id="nickname" name="nickname" class="col-xs-10 col-sm-10 " value="@Model.nickname"></div>
			<label class="col-xs-2 control-label " >性别</label>
			<div class="col-xs-4"><input type="text" id="sex" name="sex" class="col-xs-10 col-sm-10 " value="@Model.sex"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >城市</label>
			<div class="col-xs-4"><input type="text" id="city" name="city" class="col-xs-10 col-sm-10 " value="@Model.city"></div>
			<label class="col-xs-2 control-label " >国家</label>
			<div class="col-xs-4"><input type="text" id="country" name="country" class="col-xs-10 col-sm-10 " value="@Model.country"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >省份</label>
			<div class="col-xs-4"><input type="text" id="province" name="province" class="col-xs-10 col-sm-10 " value="@Model.province"></div>
			<label class="col-xs-2 control-label " >语言</label>
			<div class="col-xs-4"><input type="text" id="language" name="language" class="col-xs-10 col-sm-10 " value="@Model.language"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >表头图片地址</label>
			<div class="col-xs-4"><input type="text" id="headimgurl" name="headimgurl" class="col-xs-10 col-sm-10 " value="@Model.headimgurl"></div>
			<label class="col-xs-2 control-label " >订阅时间</label>
			<div class="col-xs-4"><input type="text" id="subscribe_time" name="subscribe_time" class="col-xs-10 col-sm-10 " value="@Model.subscribe_time"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >编号对应客户表或者维护表外键</label>
			<div class="col-xs-4"><input type="text" id="code" name="code" class="col-xs-10 col-sm-10 " value="@Model.code"></div>
			<label class="col-xs-2 control-label " >用户类型（客户、维护）</label>
			<div class="col-xs-4"><input type="text" id="userType" name="userType" class="col-xs-10 col-sm-10 " value="@Model.userType"></div>
		</div>
		<div class="clearfix form-actions"><div class="col-md-offset-5 col-md-9"><button class="btn btn-info" type="button" onclick="zstools.save()"><i class="ace-icon fa fa-check bigger-110"></i>Save（保存）</button></div></div>
	</form>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- /.row -->
    </div>
    <!-- /.page-content -->
</div>
 