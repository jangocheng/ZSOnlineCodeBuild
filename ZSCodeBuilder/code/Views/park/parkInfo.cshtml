@model Model.tb_park
<div class="main-content">
    <div class="main-content-inner">
        <div class="page-content main-content">
            <div class="breadcrumbs ace-save-state" id="breadcrumbs">
                <ul class="breadcrumb">
                    <li>
                        <i class="ace-icon fa fa-home home-icon"></i>
                        <a href="/">首页</a>
                    </li>
                    <li>
                        <a href="/park/parkList">车位信息</a>
                    </li>
                    <li class="active">
                        车位信息详情
</li>
                </ul>
                <!-- /.breadcrumb -->
            </div>

            <div class="row">
                <div class="col-xs-12">
                    <div class="widget-box">
                        <div class="widget-header">
                            <h4 class="widget-title">基本信息</h4>

                            <div class="widget-toolbar">
                                <a href="#" data-action="collapse">
                                    <i class="ace-icon fa fa-chevron-up"></i>
                                </a>
                            </div>
                        </div>

                        <div class="widget-body" style="display: block;">
                            <div class="widget-main">
                                	<form action="/park/parkSave" class="form-horizontal" role="form">
		<input type="hidden" name="id" value="@Model.id" />
		<div class="form-group">
			<label class="col-xs-2 control-label " >楼宇</label>
			<div class="col-xs-4"><input type="text" id="buildingid" name="buildingid" class="col-xs-10 col-sm-10 " value="@Model.buildingid"></div>
			<label class="col-xs-2 control-label " >停车场名称</label>
			<div class="col-xs-4"><input type="text" id="parkname" name="parkname" class="col-xs-10 col-sm-10 " value="@Model.parkname"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >停车场位置</label>
			<div class="col-xs-4"><input type="text" id="address" name="address" class="col-xs-10 col-sm-10 " value="@Model.address"></div>
			<label class="col-xs-2 control-label " >总车位数量</label>
			<div class="col-xs-4"><input type="text" id="parknumber" name="parknumber" class="col-xs-10 col-sm-10 " value="@Model.parknumber"></div>
		</div>
		<div class="form-group">
			<label class="col-xs-2 control-label " >收费标准</label>
			<div class="col-xs-4"><input type="text" id="fee" name="fee" class="col-xs-10 col-sm-10 " value="@Model.fee"></div>
			<label class="col-xs-2 control-label " >是否开放</label>
			<div class="col-xs-4"><input type="text" id="isopen" name="isopen" class="col-xs-10 col-sm-10 " value="@Model.isopen"></div>
		</div>
		<div class="clearfix form-actions"><div class="col-md-offset-5 col-md-9"><button class="btn btn-info" type="button" onclick="zstools.save()"><i class="ace-icon fa fa-check bigger-110"></i>Save（保存）</button></div></div>
	</form>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- /.row -->
    </div>
    <!-- /.page-content -->
</div>
 